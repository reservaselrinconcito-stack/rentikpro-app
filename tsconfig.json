{
  "compilerOptions": {
    "target": "ES2022",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "moduleDetection": "force",
    "allowJs": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "playwright-report",
    "test-results",
    "src-tauri/target",
    "src-tauri/target/**",
    "worker-availability/**",
    "worker-public-availability/**"
  ]
}
